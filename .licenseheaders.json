{
    "paths": ["crates/**/*.rs", "**/*.sh"],
    "exclude": ["**/node_modules/**", "**/dist/**", "**/build/**", "**/*.min.js", "**/target/**"],
    "extensions": [".rs", ".sh"],
    "comment_style": {
        ".rs": {
            "line_comment": "//"
        },
        ".sh": {
            "line_comment": "#"
        }
    }
}
